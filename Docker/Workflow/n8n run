docker run -it -d \
    --name n8n \
    -p 5678:5678 \
    -e N8N_SMTP_SENDER=n8n@admin.com \
    -e N8N_SMTP_SSL=true \
    -e GENERIC_TIMEZONE=Asia/Ho_Chi_Minh \
    -v ~/.n8n:/home/node/.n8n \
    n8nio/n8n

